{
    "name": "radix",
    "displayName": "radix",
    "description": "a diff base",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.93.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "radix.startup",
                "title": "Radix: Startup"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "vscode:prepublish": "npm run compile"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/vscode": "^1.93.0",
        "typescript": "^5.4.5"
    }
}
